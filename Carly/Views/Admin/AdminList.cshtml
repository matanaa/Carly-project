@model Carly.Models.Admin

@{
    ViewBag.Title = "Index";
}

<h2>Users and Roles</h2>
 <br /><br />

<div id="tableShow">
    <table id="Users" class="table table-hover table-bordered " cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.userName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.roleName)
                </th>
            </tr>
        </thead>
       <tbody>
    @foreach (var item in (ViewBag.UserList))
    {
        {
            <tr>
                <td>
                    @(item.userName)
                </td>
                <td>
                    @(item.email)
                </td>
                <td>
                    @(item.roleName)
                </td>
          </tr>
        }
    }
       </tbody>
    </table>


    @section scripts
{
        <script>
            $(document).ready(function () {
                $('#Users').DataTable();
            });
        </script>
    }
</div>

